{
  "version": 2,
  "builds": [{
    "src": "soozhub/wsgi.py",
    "use": "@vercel/python",
    "config": {
      "buildCommand": "python manage.py collectstatic --noinput"
    }
  }],
  "routes": [
    {"src": "/static/(.*)", "dest": "/static/$1"},
    {"src": "/(.*)", "dest": "/soozhub/wsgi.py"}
  ]
}
