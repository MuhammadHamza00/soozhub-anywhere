{
  "version": 2,
  "builds": [{
    "src": "soozhub/wsgi.py",
    "use": "@vercel/python",
    "config": { "buildCommand": "pip install -r requirements.txt && python manage.py collectstatic --noinput" }
  }],
  "routes": [ {
    "src": "/static/(.*)",
    "headers": { "cache-control": "s-maxage=31536000,immutable" },
    "dest": "/static/$1"
  }, {
    "src": "/(.*)",
    "dest": "soozhub/wsgi.py"
  }]
}
