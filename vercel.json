{
  "builds": [
    {
      "src": "soozhub/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.11",
        "buildCommand": "python manage.py collectstatic --noinput"
      }
    }
  ],
  "routes": [
      {
      "src": "/(.*)",
      "dest": "soozhub/wsgi.py"
      }
  ]
}